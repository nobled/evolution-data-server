<oaf_info>

<oaf_server iid="OAFIID:GNOME_Evolution_DataServer_BookFactory:@VERSION@"
            type="exe"
            location="@LIBEXECDIR@/evolution-data-server-@VERSION@">

	<oaf_attribute name="repo_ids" type="stringv">
		<item value="IDL:GNOME/Evolution/DataServer/BookFactory:@VERSION@"/>
		<item value="IDL:Bonobo/Unknown:1.0"/>
	</oaf_attribute>

	<oaf_attribute name="name" type="string"
                       _value="Evolution Addressbook file backend"/>

        <oaf_attribute name="addressbook:supported_protocols" type="stringv">
		<item value="file"/>
		<item value="vcf"/>
		<item value="groupwise"/>
        </oaf_attribute>
</oaf_server>

